{
  "name": "atom-ooc",
  "main": "lib/providers",
  "version": "0.1.0",
  "description": "Complete ooc support for Atom Editor",
  "repository": "https://github.com/madcapjake/atom-ooc",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-linter": "^4.3.2",
    "atom-package-deps": "^3.0.7"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "babel-eslint": "^4.1.5",
    "eslint-config-airbnb": "^3.0.2"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "no-console": 0
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
